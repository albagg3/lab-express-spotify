<section class="collection">
    {{#each artists }}
    <div class="card">
        <img src="{{this.images.1.url}}" alt="{{this.name}}">
        <div class="title-btn">
            <h3 class="title">{{ this.name }} </h3>
            <a href="/albums/{{this.id}}"><button class="btn">View Albums</button></a>
        </div>
    </div>
    {{/each }}
</section>